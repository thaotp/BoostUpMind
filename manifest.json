{
    "manifest_version":         2,
    "icons": {
      "128": "brand.png"
    },
    "permissions": [
       "storage", "https://jp-learn.herokuapp.com/api/v1/randoms/boost"
     ],
    "description":              "BoostUp",
    "name":                     "BoostUp",
    "version":                  "1",
    "content_security_policy":  "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "browser_action": {
      "default_icon": "brand.png",
      "default_popup": "popup.html"
    }
}